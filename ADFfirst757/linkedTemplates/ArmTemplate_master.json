{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ADFfirst757"
		},
		"LS_SQL_CustDB_SRC_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_SQL_CustDB_SRC'"
		},
		"LS_SQL_ScmDB_TGT_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_SQL_ScmDB_TGT'"
		},
		"LS_Storage_SADFpractice_ADLgen2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_Storage_SADFpractice_ADLgen2'"
		},
		"LS_Storage_SADFpractice_ADLgen2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://sadfpractice.dfs.core.windows.net/"
		},
		"TRG_File_Event_properties_pl_copy_file_dynamic_parameters_Filename": {
			"type": "string",
			"defaultValue": "Product.txt"
		},
		"TRG_File_Event_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/417b3563-ccf8-426e-9e2b-fbb495956f75/resourceGroups/RG_ADF_PRACTICE/providers/Microsoft.Storage/storageAccounts/sadfpractice"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ADFfirst757_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_SQL_CustDB_SRC_connectionString": {
						"value": "[parameters('LS_SQL_CustDB_SRC_connectionString')]"
					},
					"LS_SQL_ScmDB_TGT_connectionString": {
						"value": "[parameters('LS_SQL_ScmDB_TGT_connectionString')]"
					},
					"LS_Storage_SADFpractice_ADLgen2_accountKey": {
						"value": "[parameters('LS_Storage_SADFpractice_ADLgen2_accountKey')]"
					},
					"LS_Storage_SADFpractice_ADLgen2_properties_typeProperties_url": {
						"value": "[parameters('LS_Storage_SADFpractice_ADLgen2_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "ADFfirst757_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"TRG_File_Event_properties_pl_copy_file_dynamic_parameters_Filename": {
						"value": "[parameters('TRG_File_Event_properties_pl_copy_file_dynamic_parameters_Filename')]"
					},
					"TRG_File_Event_properties_typeProperties_scope": {
						"value": "[parameters('TRG_File_Event_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'ADFfirst757_ArmTemplate_0')]"
			]
		}
	]
}